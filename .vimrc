set backspace=indent,eol,start
syntax enable
set showmode
set number
set showcmd
set wildmenu
runtime ftplugin/man.vim
set autoindent
set tabstop=4
set shiftwidth=4
set expandtab
set softtabstop=4
set hlsearch
set showmatch
set ignorecase
set smartcase
set laststatus=2
set statusline=%F%m%r%h%w\ [FORMAT=%{&ff}]\ [TYPE=%Y]\ [ASCII=\%03.3b]\ [POS=%04l,%04v][%p%%]\ [LEN=%L]
filetype plugin on
